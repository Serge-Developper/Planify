{
  "version": 2,
  "functions": {
    "api/users/login.js": {
      "runtime": "nodejs22.x"
    },
    "api/users/secret-questions.js": {
      "runtime": "nodejs22.x"
    },
    "api/users/check-secret-questions.js": {
      "runtime": "nodejs22.x"
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
