{
  "version": 2,
  "buildCommand": "cd frontend-clean/frontend && npm install && npm run build",
  "outputDirectory": "frontend-clean/frontend/dist",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
